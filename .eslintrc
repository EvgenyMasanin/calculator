{
  "env": {
    "browser":true,
    "es6": true
  },
  "extends": [
    "airbnb-base", "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style":"off",
    "default-case":"off",
    "max-classes-per-file": "off",
    "class-methods-use-this":"off",
    "no-unused-expressions":"off",
    "no-plusplus":"off",
    "no-eval":"off"
  }
}
